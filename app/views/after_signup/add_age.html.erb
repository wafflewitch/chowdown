<div class="text-center form-signup">
<h4 class="text-signup">How old are you?</h4>
<%= simple_form_for @user, url: wizard_path do |f| %>
  <%= f.text_field :age, required: true, class: "form-control form-center" %><br>
  <%= f.submit "Continue", class: "btn button-chowdowns button-onboarding" %>
<% end %>
</div>


<% content_for(:center_nav) do %>
  <%= image_tag "chowdown.png", alt: "title logo", class: "navbar-logo"%>
<% end %>
